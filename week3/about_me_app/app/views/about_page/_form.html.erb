

<%= form_with(url: "/about_page/about_page", method: "post") do %>
        <%= label_tag(:name, "NAME") %><br>
        <%= text_field_tag(:name) %> <br><br>
        <%= label_tag(:email, "EMAIL") %><br>
        <%= text_field_tag(:email) %> <br><br><br>
        <%= submit_tag("Submit") %>
<% end %>